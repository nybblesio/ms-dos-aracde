AS=a86
AS_FLAGS=DEBUG
GAME_SRCS=game.8
BANKED_SRCS=banked.8

all: game banked

game: $(GAME_SRCS)
    $(AS) =$(AS_FLAGS) $(GAME_SRCS)

banked: $(BANKED_SRCS)
    $(AS) =$(AS_FLAGS) $(BANKED_SRCS)

clean:
    del *.obj
    del *.com
    del *.exe
    del *.lst
    del *.sym
    del *.xrf
    del *.err
    del *.bak
    del *.old
    del *.und
